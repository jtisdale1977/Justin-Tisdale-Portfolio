                    <!-- jquery function for Factorial Modal -->
                    <script>
                        $("#newfact").click(function () {
                            var factorial = Number($("#factorial").val());
                            tempvalue = 1;
                            if ($.trim($("#resultFact").text()) != "") {
                                $("#resultFact").empty();
                            }
                            if ((factorial == 0)) {
                                $("#errorFact").empty();
                                $("#errorFact").append("<p>Really?!? Enter a number please!</p>");
                            }
                            else if (factorial < 0) {
                                $("#errorFact").empty();
                                $("#errorFact").append("<p>Try being more positive instead of so negative!</p>");
                            }
                            else if (factorial > 170) {
                                $("#errorFact").empty();
                                $("#errorFact").append("<p>This number's factor is too long to display!</p>");
                            }
                            else if ((factorial % 1) != 0) {
                                $("#errorFact").empty();
                                $("#errorFact").append("<p>um...Whole numbers please!</p>");
                            }
                            else {
                                if (factorial >= 0) {
                                    $("#errorFact").empty();
                                    for (i = factorial ; i > 0 ; i--) {
                                        tempvalue *= i;
                                    }
                                    $("#resultFact").text(tempvalue);
                                 }
                            }
                        });
                    </script>
