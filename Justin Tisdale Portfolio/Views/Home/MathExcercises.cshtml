                                            <pre class="brush: js;">
												<!-- place Factorial formula here for syntaxhighlighter -->
												$("#newfact").click(function () {
												var factorial = Number($("#factorial").val());
												tempvalue = 1;
												for (i = factorial ; i > 0 ; i--) {
													tempvalue *= i;
												}
												$("#resultFact").text(tempvalue);
														});
												<!-- place Factorial formula here for syntaxhighlighter -->
											</pre>
